# GEOINT Service Configuration
# Copy this file to .env and fill in your values

# Application
APP_ENV=development
DEBUG=true

# Database - PostGIS
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=geoint
POSTGRES_USER=geoint
POSTGRES_PASSWORD=your_secure_password_here

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# MinIO / S3 Storage
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=your_secure_key_here
MINIO_SECURE=false

# Satellite Providers
# Sentinel Hub (Copernicus) - Get credentials at https://scihub.copernicus.eu/
SENTINEL_HUB_CLIENT_ID=
SENTINEL_HUB_CLIENT_SECRET=
SENTINEL_HUB_ENABLED=true

# Planet Labs - Get API key at https://www.planet.com/
PLANET_API_KEY=
PLANET_ENABLED=false

# Maxar - Contact sales for API access
MAXAR_API_KEY=
MAXAR_ENABLED=false

# Capella Space SAR - Contact sales for API access
CAPELLA_API_KEY=
CAPELLA_ENABLED=false

# ML Models
YOLO_MODEL_PATH=models/yolov8_satellite.pt
DETECTION_CONFIDENCE=0.25
DETECTION_IOU_THRESHOLD=0.45

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# PHP Backend Integration
PHP_BACKEND_URL=http://localhost:8080
PHP_AUTH_SECRET=change_this_shared_secret

# Tile Server
TILE_CACHE_TTL=3600
MAX_TILE_SIZE=512
